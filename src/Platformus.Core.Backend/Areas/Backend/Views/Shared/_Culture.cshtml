@model Platformus.Core.Backend.ViewModels.Shared.CultureViewModel
@inject Microsoft.AspNetCore.Mvc.Localization.IViewLocalizer Localizer
<row>
  <cell>@Model.Id</cell>
  <cell>@Model.Name</cell>
  <buttons-cell>
    <positive-button is-minor="true" href="/backend/cultures/edit/@Model.Id">@Localizer["Edit"]</positive-button>
    <delete-button href="/backend/cultures/delete/@Model.Id"></delete-button>
  </buttons-cell>
</row>