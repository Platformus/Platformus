@model Platformus.Website.Backend.ViewModels.Shared.DataSourceViewModel
@inject Microsoft.AspNetCore.Mvc.Localization.IViewLocalizer Localizer
<row>
  <cell>@Model.Code</cell>
  <cell>@Model.DataProviderCShartClassName</cell>
  <buttons-cell>
    <positive-button is-minor="true" href="/backend/datasources/edit/@Model.Id">@Localizer["Edit"]</positive-button>
    <delete-button href="/backend/datasources/delete/@Model.Id"></delete-button>
  </buttons-cell>
</row>