@model Platformus.Website.Backend.ViewModels.Shared.CompletedFormViewModel
@inject Microsoft.AspNetCore.Mvc.Localization.IViewLocalizer Localizer
<row>
  <cell>@Model.Form.Name</cell>
  <cell>@Model.Created.ToString()</cell>
  <buttons-cell>
    <positive-button is-minor="true" href="/backend/completedforms/view/@Model.Id">@Localizer["View"]</positive-button>
    <delete-button href="/backend/completedforms/delete/@Model.Id"></delete-button>
  </buttons-cell>
</row>